    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: raspapi Debug",
            "type": "coreclr",
            "request": "launch",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "program": "${workspaceFolder}/bin/Debug/net8.0/raspapi.dll",
            "preLaunchTask": "builds"
        },
        {
            "name": "C#: raspapi Production",
            "type": "coreclr",
            "request": "launch",
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Production"
            },
            "program": "${workspaceFolder}/bin/Debug/net8.0/raspapi.dll",
            "preLaunchTask": "builds"
        },
