{
    "BRIAN_TEST": "prod",
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        },
        "Console": {
            "FormatterName": "simple",
             "FormatterOptions": {
                "SingleLine": true,
                "IncludeScopes": true,
                "TimestampFormat": "yyyy-MM-dd HH:mm:ss zzz ",
                "ColorBehavior": "enabled"
            }
        }
    },
    "AllowedHosts": "*"
}